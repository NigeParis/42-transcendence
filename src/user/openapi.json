                                                "msg": {
                                                    "enum": [
                                                        "auth.noCookie",
                                                        "auth.invalidKind",
                                                        "auth.noUser",
                                                        "auth.invalid"
                                                    ]
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
